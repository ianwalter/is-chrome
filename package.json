{
  "name": "@ianwalter/is-chrome",
  "version": "0.0.0",
  "description": "Detect whether a browser environment or user agent supports input[type=date]",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Ian Walter",
    "url": "https://ianwalter.dev"
  },
  "main": "dist/is-chrome.js",
  "module": "dist/is-chrome.m.js",
  "browser": "dist/is-chrome.browser.js",
  "scripts": {
    "lint": "eslint .",
    "nodejs": "dist -i src/node.js --no-browser",
    "browser": "dist -i src/browser.js --no-esm --no-cjs",
    "pretest": "yarn nodejs && yarn browser",
    "test": "bff"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/is-chrome.git"
  },
  "keywords": [
    "date",
    "input",
    "support",
    "detection",
    "browser",
    "useragent",
    "user",
    "agent"
  ],
  "bugs": {
    "url": "https://github.com/ianwalter/is-chrome/issues"
  },
  "homepage": "https://github.com/ianwalter/is-chrome#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "bowser": "^2.9.0"
  },
  "devDependencies": {
    "@ianwalter/bff": "^9.6.0",
    "@ianwalter/bff-puppeteer": "^1.4.0",
    "@ianwalter/dist": "^3.0.0",
    "@ianwalter/eslint-config": "^4.0.0",
    "@ianwalter/release": "^5.0.0",
    "@ianwalter/renovate-config": "^1.2.0"
  },
  "bff": {
    "plugins": [
      "@ianwalter/bff-puppeteer"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@ianwalter/eslint-config"
    ],
    "globals": {
      "isChrome": true
    }
  },
  "eslintIgnore": [
    "dist"
  ],
  "renovate": {
    "extends": [
      "@ianwalter"
    ]
  }
}
